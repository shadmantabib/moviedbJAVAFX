      <ChoiceBox fx:id="choice" layoutX="253.0" layoutY="1.0" prefWidth="150.0" style="-fx-background-color: indigo;" />
      <Button fx:id="backing" layoutX="652.0" layoutY="2.0" mnemonicParsing="false" onAction="#Exit" prefHeight="26.0" prefWidth="57.0" style="-fx-background-color: red;" text="Back" />
   </children>
</AnchorPane>
