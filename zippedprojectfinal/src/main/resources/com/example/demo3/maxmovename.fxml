<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo3.maxmovenamecontroller">
   <children>
      <VBox layoutY="30.0" prefHeight="800.0" prefWidth="1000.0">
         <children>
            <TableView fx:id="table" prefHeight="800.0" prefWidth="1000.0">
              <columns>
                <TableColumn fx:id="titleCol" prefWidth="123.00000154972076" style="-fx-background-color: grey;" text="Title" />
                <TableColumn fx:id="ryearCol" prefWidth="112.99999237060547" style="-fx-background-color: #bb86d9;" text="Release Year" />
                  <TableColumn fx:id="genreCol" prefWidth="285.6000061035156" style="-fx-background-color: grey;" text="Genre" />
                  <TableColumn fx:id="durCol" prefWidth="107.20001220703125" style="-fx-background-color: #bb86d9;" text="Duration" />
                  <TableColumn fx:id="prodCol" prefWidth="178.79998779296875" style="-fx-background-color: grey;" text="Prod Company" />
                  <TableColumn fx:id="budCol" prefWidth="92.0" style="-fx-background-color: #bb86d9;" text="Budget" />
                  <TableColumn fx:id="revCol" prefWidth="89.39996337890625" style="-fx-background-color: grey;" text="Revenue" />
              </columns>
            </TableView>
         </children>
      </VBox>
      <Button fx:id="loadVar" layoutX="14.0" layoutY="2.0" mnemonicParsing="false" onAction="#onLoadclick" prefHeight="26.0" prefWidth="138.0" style="-fx-background-color: red;" text="Load All Movies">
